package structures;

/**
 * Abstract data type used to add LinkedList node to front of queue.
 * 
 * @author David Saelee
 * @version 01/26/2019
 *
 * @param <T> Generic parameter type.
 */
public interface OutputRestrictedDequeADT<T> extends QueueADT<T> {
    /**
     * Method signature to add element to front of queue.
     * 
     * @param theElement generic type parameter.
     */
    void enqueueAtFront(T theElement);

}
